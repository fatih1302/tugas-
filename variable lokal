#include <iostream>
using namespace std;

//Tugas variable lokal,fatih

int ContohFunction1 () 
{
	int q = 9 ;
	return q;
	//variable dihancurkan/dihapus dari alamat memori
}
int ContohFunction2 () 
{
	int p = 8 ;
	return p;
	//variable dihancurkan/dihapus dari alamat memori	
}
int main ()
{
	cout << "TUGAS VARIABLE LOKAL,M FATIH A" << endl;
	cout << endl;
	
	cout << "Ini tampilan dari Variable Lokal:" <<endl;
	int q = 1 ;
	cout << "Ini variable lokal dari fungsi utama adalah " << q << endl;
	cout << "Ini variable lokal dari fungsi Contoh Function q adalah " << ContohFunction1 ();
	cout << endl;
	int p = 3 ;
	cout << "Ini variable lokal dari fungsi kedua adalah " << p << endl;
	cout << "Ini variable lokal dari fungsi Contoh Function p adalah " << ContohFunction2 ();
	cout << endl;
}
